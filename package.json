{
  "name": "@ng-states/core",
  "version": "1.0.0",
  "description": "A modern TypeScript library for Nigerian states, local governments, and senatorial districts with zero dependencies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "npm run build && mocha -r ts-node/register test/**/*.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eminisolomon/naija-state-local-government.git"
  },
  "keywords": [
    "Nigeria",
    "States",
    "Local Government",
    "LGA",
    "Senatorial Districts",
    "TypeScript",
    "Nigerian States",
    "Naija",
    "Geography",
    "Administrative Divisions"
  ],
  "author": "Solomon Olatunji (@eminisolomon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eminisolomon/naija-state-local-government/issues"
  },
  "homepage": "https://github.com/eminisolomon/naija-state-local-government#readme",
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.7",
    "assert": "^1.4.1",
    "mocha": "^10.2.0",
    "react": "^19.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vue": "^3.5.25"
  }
}
